@import "variables.styl"

base-container()
    .todo-container
        width base-width
        margin 60px auto 0
        border-radius 4px
        box-shadow rgba(99, 99, 99, 0.2) 0 2px 8px 0
        padding 25px

        .todo-wrapper
            border: 1px solid #ccc
            border-radius 4px
            margin: 0 auto